<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.NeuroLogicNew" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/toolbar_background</item>
        <item name="colorPrimaryVariant">@color/toolbar_background2</item>
        <item name="colorOnPrimary">@color/text</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">@color/text</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.NeuroLogicNew.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.NeuroLogicNew.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.NeuroLogicNew.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>